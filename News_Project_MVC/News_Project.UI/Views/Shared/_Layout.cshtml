    <header>
        <div class="main-menu" id="main-menu">
            <div class="container">
                <div class="row align-items-center justify-content-between">
                    <div>
                        <a class="navbar-brand"href="~/Views/Home/Index.cshtml">
                            <img src="~/Assets/img/logo.png" alt="">
                        </a>
                    </div>
                    <nav id="nav-menu-container" class="ml-auto">
                        <ul class="nav-menu">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("Archive","Archive","Archive")</li>
                            <li class="menu-has-children">
                                <a href="">Categories</a>
                                <ul>
                                    <li>@Html.ActionLink("Spor", "Archive", "Archive", new { CategoryNo = 1 }, null)</li>
                                    <li>@Html.ActionLink("Finans", "Archive", "Archive", new { CategoryNo = 2 }, null)</li>
                                    <li>@Html.ActionLink("Ekonomi", "Archive", "Archive", new { CategoryNo = 3 }, null)</li>
                                    <li>@Html.ActionLink("Magazin", "Archive", "Archive", new { CategoryNo = 4 }, null)</li>
                                    <li>@Html.ActionLink("Dünya", "Archive", "Archive", new { CategoryNo = 5 }, null)</li>
                                    
                                </ul>
                            </li>
                            
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                    </nav>
                    <div class="navbar-right ml-auto">
                        <div class="row">
                            <div class="col-6">
                                <button class="btn btn-lg btn-outline-primary btn-block" style="width:150px;margin-top:25px;" type="submit"><i class="fa fa-sign-in"></i>@Html.ActionLink("Login In", "Login", "Login")</button>
                            </div>

                        </div>
                </div>
            </div>
        </div>
    </header>
    @RenderBody()
