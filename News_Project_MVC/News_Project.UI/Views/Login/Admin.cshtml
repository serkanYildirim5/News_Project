@model News_Project.UI.Models.AdminModel
@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_LayoutControlPanel.cshtml";
}

<h2>Hoşgeldin Admin</h2>

<table id="mytable" class="table table-bordred table-striped" style="margin-top:200px">

    <thead>

    <th><input type="checkbox" id="checkall" /></th>
    <th>Id</th>
    <th>FullName</th>
    <th>Email</th>
    <th>Password</th>
    <th>RoleId</th>
    <th>Edit</th>
    <th>Delete</th>
   
    <tbody>
        @foreach (var item in Model.Users)
        {
            <tr>
                <td><input type="checkbox" class="checkthis" /></td>

                @Html.Partial("_PartialUserEdit", item)
                <td>
                    <p data-placement="top" data-toggle="tooltip" title="Delete">
                        <button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete">
                            @Html.ActionLink("Delete", "UserDelete", "Login", item, new { @class = "btn btn-danger btn-xs" })
                        </button>
                    </p>
                </td>
            </tr>
        }






    </tbody>

</table>